{
  "name": "@apeiwan/ppjsbridge",
  "version": "0.1.5",
  "description": "皮皮PiPi App webview jsbridge",
  "main": "dist/ppjsbridge.js",
  "namespace": "PPJSBridge",
  "aliasOSS": "ppjsbridge",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:oss": "rm -rf docs/.vuepress/dist && npm run docs:build && node build/docs.oss.js",
    "build:beta:released": "npm run rollup:beta && npm run build:prod",
    "build:beta:released:oss": "npm run rollup:beta:oss && npm run rollup:prod:oss ",
    "build:beta": "npm run rollup:beta",
    "build:beta:oss": "npm run rollup:beta && npm run npm:oss:beta",
    "build:released": "npm run rollup:prod",
    "build:released:oss": "npm run rollup:prod && npm run npm:oss:prod",
    "clear": "rm -rf dist",
    "rollup:prod": "npm run clear && rollup -c --format umd --tag released && rollup -c --format umd --uglify true --tag released",
    "rollup:beta": "npm run clear && rollup -c --format umd --tag beta && rollup -c --format umd --uglify true --tag beta",
    "npm:oss:prod": "node build/npm.oss.js && node build/npm.oss.js --tag released",
    "npm:oss:beta": "node build/npm.oss.js --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apeiwan/ppjsbridge.git"
  },
  "keywords": [
    "pipiapp",
    "jsbridge"
  ],
  "author": "apeiwan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apeiwan/ppjsbridge/issues"
  },
  "homepage": "http://github-npm.apeiwan.com/ppjsbridge/docs/index.html",
  "dependencies": {
    "wolfy87-eventemitter": "^5.2.9"
  },
  "files": [
    "dist",
    "README.MD",
    "package.json",
    "types"
  ],
  "typings": "types/index.d.ts",
  "devDependencies": {
    "@apeiwan/ppjsbridge": "^0.1.3",
    "@vuepress/plugin-active-header-links": "^1.3.0",
    "@vuepress/plugin-back-to-top": "^1.3.0",
    "ali-oss": "^6.5.1",
    "dayjs": "^1.8.22",
    "minimist": "^1.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "shelljs": "^0.8.3",
    "vant": "^2.4.7",
    "vconsole": "^3.3.4",
    "vue-clipboard2": "^0.3.1",
    "vuepress": "^1.3.0",
    "vuepress-plugin-container": "^2.1.2"
  }
}
